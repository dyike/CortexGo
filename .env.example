# CortexGo Environment Configuration Template
# Copy this file to .env and fill in your actual values


# Data Sources
FINNHUB_API_KEY=your_finnhub_api_key_here

# Longport API Configuration
LONGPORT_APP_KEY=your_longport_app_key_here
LONGPORT_APP_SECRET=your_longport_app_secret_here
LONGPORT_ACCESS_TOKEN=your_longport_access_token_here

# Reddit API Configuration
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_SECRET=your_reddit_secret
REDDIT_USER_AGENT=CortexGo/1.0

# System Configuration
MAX_DEBATE_ROUNDS=3
MAX_RISK_DISCUSS_ROUNDS=2
MAX_RECUR_LIMIT=100

# Features
ONLINE_TOOLS=true
DEBUG=false
CACHE_ENABLED=true

# Eino Debug Configuration
EINO_DEBUG_ENABLED=false
EINO_DEBUG_PORT=52538
